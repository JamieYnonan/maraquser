{
    "name": "jamieynonan/maraquser",
    "description": "Maraquser, project to manage users roles and permissions.",
    "type": "project",
    "license": "MIT",
    "keywords": [],
    "authors": [
        {
            "name": "Jamie Ynonan",
            "email": "jamiea31@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "doctrine/dbal": "^2.9",
        "doctrine/orm": "^2.6",
        "firebase/php-jwt": "^4.0",
        "jamieynonan/base-value-object": "^1.1",
        "jamieynonan/default-property-normalizer": "^1.0",
        "jamieynonan/base-value-object-normalizer": "^1.0",
        "league/tactician": "^1.0",
        "league/tactician-doctrine": "^1.1",
        "league/event": "^2.2",
        "monolog/monolog": "^1.24",
        "nesbot/carbon": "^2.18",
        "php-amqplib/php-amqplib": "^2.8",
        "phpseclib/phpseclib": "^2.0",
        "ramsey/uuid": "^3.8",
        "symfony/dependency-injection": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/http-foundation": "^4.2",
        "symfony/proxy-manager-bridge": "^4.2",
        "symfony/routing": "^4.2",
        "symfony/serializer": "^4.2",
        "symfony/symfony": "^4.2",
        "symfony/yaml": "^4.2",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "infection/infection": "^0.12.2",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/console": "^4.2",
        "symfony/debug": "^4.2",
        "symfony/var-dumper": "^4.2"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Mu\\": "src/Mu/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mu\\": "tests/Mu/"
        },
        "files": ["vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"]
    },
    "scripts": {
        "test": "phpunit",
        "infection": "infection --coverage=build/coverage --only-covered --threads=4",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "doctrine": "doctrine"
    },
    "config": {
        "sort-packages": true
    }
}
